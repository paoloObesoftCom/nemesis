<setupxml>
	<setup key="osd" title="Setup InfoBar OSD">
		<item level="0" text="Show Extended InfoBar Permanent">config.nemesis.einfo</item>
		<item level="0" text="Replace InfoBar with Extended InfoBar">config.nemesis.replaceinfobar</item>
		<item level="0" text="Extended InfoBar delay (msec)">config.nemesis.einfotimeout</item>
		<item level="0" text="Extended InfoBar delay on zap (msec)">config.nemesis.eiinfobardelayonzap</item>
		<item level="0" text="Infobar timeout (sec)">config.usage.infobar_timeout</item>
		<item level="0" text="Show infobar on channel change">config.usage.show_infobar_on_zap</item>
		<item level="0" text="Show infobar on skip forward/backward">config.usage.show_infobar_on_skip</item>
		<item level="0" text="Show infobar on event change">config.usage.show_infobar_on_event_change</item>
		<item level="0" text="Show decode network details">config.nemesis.shownetdet</item>
		<item level="0" text="EMM Info delay (msec)">config.nemesis.emminfodelay</item>
		<item level="0" text="ECM Info delay (msec)">config.nemesis.ecminfodelay</item>
		<item level="0" text="Set InfoBar Picon type">config.nemesis.picontype</item>
		<item level="0" text="Enable fade in of InfoBar">config.plugins.FadeSet.fadeIn</item>
		<item level="0" text="Enable fade out of InfoBar">config.plugins.FadeSet.fadeOut</item>
		<item level="0" text="Enable fade in of InfoBar on Zap">config.plugins.FadeSet.fadeInOnZap</item>
		<item level="0" text="Speed for fade in/out of InfoBar (1-10)">config.plugins.FadeSet.timeout</item>
	</setup>
	<setup key="ipkgop" title="Setup OPKG Options">
		<item level="0" text="Force overwrite on upgrade">config.nemesis.ipkg.overwriteUpgrade</item>
		<item level="0" text="Force reinstall on install">config.nemesis.ipkg.forceReInstall</item>
		<item level="0" text="OPKG Update connection timeout (20-60 sec)">config.nemesis.ipkg.updateTimeout</item>
	</setup>
	<setup key="dev" title="Setup Device">
		<item level="0" text="Force Umount before Format">config.nemesis.forceumount</item>
	</setup>
	<setup key="lcd" title="Setup Display LCD/OLED" requires="FrontpanelDisplay">
		<item level="0" text="Brightness">config.lcd.bright</item>
		<item level="0" text="Enable Brightness on vision">config.lcd.enablelcdbri</item>
		<item level="0" text="Brightness on vision">config.lcd.lcdbri</item>
		<item level="0" text="Contrast">config.lcd.contrast</item>
		<item level="0" text="Standby">config.lcd.standby</item>
		<item level="0" text="Show Picon on LCD? (Need GUI Restart)">config.nemesis.piconlcd</item>
		<item level="0" text="Set LCD Picon type">config.nemesis.piconlcdtype</item>
	</setup>
	<setup key="console" title="Setup Console">
		<item level="0" text="Auto close Console on finished event">config.nemesis.autocloseconsole</item>
		<item level="0" text="Delay before auto close Console (sec)">config.nemesis.autocloseconsoledelay</item>
		<item level="0" text="Enable VirtualKeyBoard (Experimental)">config.nemesis.usevkeyboard</item>
	</setup>
	<setup key="picon" title="Setup Picon">
		<item level="0" text="Show Picon on LCD? (Need GUI Restart)">config.nemesis.piconlcd</item>
		<item level="0" text="Set LCD Picon type">config.nemesis.piconlcdtype</item>
		<item level="0" text="Set InfoBar Picon type">config.nemesis.picontype</item>
		<item level="0" text="Can Install Picon in HDD?">config.nemesis.usepiconinhdd</item>
	</setup>
	<setup key="inadyn" title="Setup InaDyn">
		<item level="0" text="Set user">config.inadyn.user</item>
		<item level="0" text="Set password">config.inadyn.password</item>
		<item level="0" text="Set domain name">config.inadyn.alias</item>
		<item level="0" text="Set update period (sec)">config.inadyn.period</item>
		<item level="0" text="Set DNS System">config.inadyn.system</item>
		<item level="0" text="Set log file">config.inadyn.log</item>
	</setup>
	<setup key="ipupdate" title="Setup EZ-IpUpdate">
		<item level="0" text="Set user">config.ipupdate.user</item>
		<item level="0" text="Set password">config.ipupdate.password</item>
		<item level="0" text="Set domain name">config.ipupdate.alias</item>
		<item level="0" text="Set server">config.ipupdate.server</item>
		<item level="0" text="Set update period (sec)">config.ipupdate.period</item>
		<item level="0" text="Set DNS System">config.ipupdate.system</item>
		<item level="0" text="Set log file">config.ipupdate.log</item>
	</setup>
	<setup key="proxy" title="Setup Proxy">
		<item level="0" text="Activate proxy?">config.proxy.isactive</item>
		<item level="0" text="Set Proxy Server">config.proxy.server</item>
		<item level="0" text="Set Proxy Server port">config.proxy.port</item>
		<item level="0" text="Set user">config.proxy.user</item>
		<item level="0" text="Set password">config.proxy.password</item>
	</setup>
	<setup key="epg" title="EPG Configuration">
		<item level="0" text="EPG Enigma cache save path">config.misc.epgcache_filename</item>
		<item level="0" text="Number of displayed element on search">config.nemepg.elemnum</item>
		<item level="0" text="Zap delay before download EPG">config.nemepg.zapdelay</item>
		<item level="0" text="Download EPG SKY-IT">config.nemepg.downskyit</item>
		<item level="0" text="Channel to download EPG SKY-IT">config.nemepg.skyitch</item>
		<item level="0" text="Download EPG SKY-UK">config.nemepg.downskyuk</item>
		<item level="0" text="Channel to download EPG SKY-UK">config.nemepg.skyukch</item>
		<item level="0" text="Clear enigma cache before load EPG">config.nemepg.clearcache</item>
		<item level="0" text="Clear Backup EPG after clear Enigma cache">config.nemepg.clearbackup</item>
		<item level="0" text="Enable EPG Download via Timer">config.nemepg.enatimer</item>
		<item level="0" text="Multi-EPG bouquet selection">config.usage.multiepg_ask_bouquet</item>
	</setup>
</setupxml>

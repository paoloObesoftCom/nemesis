		<script language="javascript" type="text/javascript">
			function encode(inputtext) 
			{
				var etext = "";
				for (var n = 0; n < inputtext.length; n++)
				{
					var c = inputtext.charCodeAt(n);
					if ((c > 44) && (c < 127))
						etext += String.fromCharCode(c);
					else 
					{
						etext += "%";
						etext += String.fromCharCode(48+(c / 16));
						if ((c & 15) > 9)
							etext += String.fromCharCode(55+(c & 15));
						else
							etext += String.fromCharCode(48+(c & 15));
					}
				}
				return etext;
			}
			function StartEmu(ems)
			{
				var ids = encode("/NT/startEmu?emuToStart=" + ems.options[ems.selectedIndex].value);
				NewWindow(ids, "", "500", "50", "no", "10000");
			}
			function StartCs(cs)
			{
				var ids = encode("/NT/startCs?csToStart=" + cs.options[cs.selectedIndex].value);
				NewWindow(ids, "", "500", "50", "no", "10000");
			}
			function refr()
			{
				if (document.NT.cb1.checked)
					location.reload(true);
			}
			function init2()
			{
				setTimeout("refr()", 7000);
			}
			function cb1click(cb)
			{
				if (document.NT.cb1.checked)
				{
					location.reload(true);
					setTimeout("refr()", 7000);
				}
			}
		</script>


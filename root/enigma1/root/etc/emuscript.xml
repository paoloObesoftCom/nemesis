<emuscripts>
	<script>
		<name>Show NewCS Info</name>
		<cmd>(echo ''; sleep 1; echo 'version'; sleep 1; echo 'uptime'; sleep 1; echo 'quit') | nc 127.0.0.1 3001</cmd>
		<info>Show NewCS Info</info>
	</script>
